	keywords = {SARS-CoV, 2019-nCoV, ACE2, monoclonal antibody, RBD},
	pages = {382--385},
	file = {PDF:C\:\\Users\\kalab\\Zotero\\storage\\VUKM382R\\full-text.pdf:application/pdf},
}

