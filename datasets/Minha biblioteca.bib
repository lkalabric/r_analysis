	journal = {Journal of Clinical Microbiology},
	author = {Callahan, J. D. and Wu, S. J.L. and Dion-Schultz, A. and Mangold, B. E. and Peruski, L. F. and Watts, D. M. and Porter, K. R. and Murphy, G. R. and Suharyono, W. and King, C. C. and Hayes, C. G. and Temenak, J. J.},
