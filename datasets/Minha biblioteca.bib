@article{alves_substituicao_2020,
	title = {Substituição ou compra de equipamentos médico hospitalares pela aplicação do método {AHP}},
	url = {https://repositorio.ufrn.br/handle/123456789/32855},
	abstract = {Introdução - A tecnologia tem dominado as práticas da medicina, de forma que,
praticamente, todas as especialidades médicas dependem de alguma maneira de
equipamentos médico-hospitalares. Tal dependência aumenta a necessidade de
implementar metodologias eficazes de gestão de tecnologias de forma que tornem os
processos e decisões mais assertivas. Desde 2015, quando o Hospital Universitário
de Lagarto – UFS foi incorporado pela rede EBSERH e passou a receber incentivos,
principalmente pelo Programa Nacional de Reestruturação dos Hospitais
Universitários Federais (Rehuf), foi evidenciado expressivo crescimento e renovação
do parque tecnológico do hospital com volumosas substituições de equipamentos
médico hospitalares, porém, nenhum fluxo, processo ou política de substituição de
equipamentos foi implantado ou utilizado. Objetivo - Propor e implementar uma
ferramenta para substituição de EHM a partir do método AHP no Hospital Universitário
de Lagarto. Metodologia- Tratou-se de uma pesquisa bibliográfica em que foram
utilizadas bases de dados online a fim de determinar o método a ser escolhido, estudo
e descrição do método, proposição da utilização do método AHP na problemática de
substituição de equipamentos médico-hospitalares e aplicação. Resultados - A partir
da metodologia aplicada foi possível elaborar a ferramenta proposta para tomada de
decisão para substituição de equipamentos médico-hospitalares, podendo esta
substituição ser oriunda de compra, doação ou remanejamento interno de
equipamentos no hospital, a ferramenta foi descrita em formato de manual de
instruções para aplicação do método em qualquer hospital da rede EBSERH.
Conclusão - A análise do método AHP apresentou vantagens, como simplicidade e
facilidade de aplicação, além de resposta simples e objetiva.},
	urldate = {2023-04-18},
	author = {Alves, Nathalia Rocha},
	month = dec,
	year = {2020},
	note = {Publisher: Universidade Federal do Rio Grande do Norte},
	keywords = {masterThesis, Método AHP, Substituição de equipamento médico hospitalar, Tomada de decisão},
	file = {PDF:C\:\\Users\\kalab\\Zotero\\storage\\3HWLGBVJ\\full-text.pdf:application/pdf},
