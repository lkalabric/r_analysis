	pmcid = {PMC5850084},
	pages = {ELIXIR--148},
	file = {PubMed Central Full Text PDF:C\:\\Users\\kalab\\Zotero\\storage\\AB53YKRI\\Dominguez Del Angel et al. - 2018 - Ten steps to get started in Genome Assembly and An.pdf:application/pdf},
}

