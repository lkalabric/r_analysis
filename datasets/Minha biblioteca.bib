	pages = {557--568},
	file = {PDF:C\:\\Users\\kalab\\Zotero\\storage\\U9IT72LX\\Pacheco et al. - 2020 - Predominance of Giardia duodenalis AII sub-assemblage in young children from Salvador, Bahia, Brazi.pdf:application/pdf},
