	note = {Publisher: Nature Publishing Group},
	keywords = {Genetic markers, Methods, Multivariate analysis, Ordination, Principal component analysis, Statistics},
	pages = {330--341},
	file = {PDF:C\:\\Users\\kalab\\Zotero\\storage\\9GDLM3VG\\full-text.pdf:application/pdf},
}

@article{kim_microsatellite_2013,
