	pmid = {26247383},
	note = {Publisher: Faculdade de Saúde Pública da Universidade de São Paulo},
	keywords = {Prevalence, Hepatitis C, epidemiology, Hepacivirus, Prisoners, Review, Seroepidemiologic studies},
	pages = {1--10},
