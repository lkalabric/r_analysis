	author = {Carrazzone, Cristina F V and Maria De Brito, Ana and Gomes, Yara M},
	year = {2004},
	note = {Publication Title: bras. hematol. hemoter
Volume: 26
Issue: 2},
	pages = {93--98},
	file = {PDF:C\:\\Users\\kalab\\Zotero\\storage\\XHD2XC6U\\m-api-2e60cad3-5f7f-84b3-60da-b1ce0062a31b.pdf:application/pdf},
}

