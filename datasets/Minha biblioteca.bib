	author = {Costa, Juliana dos Santos and Junior, Luiz Mauricio Lopes de Andrade},
	month = apr,
	year = {2021},
	note = {Publisher: Research, Society and Development},
	keywords = {Bibliometrics., Electricity},
	pages = {e26210414085--e26210414085},
	file = {PDF:C\:\\Users\\kalab\\Zotero\\storage\\JHGLG7HZ\\full-text.pdf:application/pdf},
}

